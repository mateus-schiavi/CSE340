<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <div id="tools">
        <% if (typeof accountData !== 'undefined' && accountData) { %>
        <a href="/account/manage" title="Account Management">Welcome <%= accountData.firstname %></a>
        <a href="/account/logout" title="Logout">Logout</a>
        <% } else { %>
        <a href="/account/login" title="Click to log in">My Account</a>
        <% } %>
    </div>
</header>